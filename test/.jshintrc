// see http://www.jshint.com/docs/options/
// be sure to turn off 'devel' before shipping.
{
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "sub": true,
  "undef": true,
  "unused": true,
  "boss": true,
  "eqnull": true,
  "browser": true,
  "quotmark": "double",
  "strict": false,
  "predef": [
    "jQuery",
    "QUnit",
    "module",
    "test",
    "asyncTest",
    "expect",
    "start",
    "stop",
    "ok",
    "equal",
    "notEqual",
    "deepEqual",
    "notDeepEqual",
    "strictEqual",
    "notStrictEqual",
    "throws",
    "sinon",
    "console"
  ]
}